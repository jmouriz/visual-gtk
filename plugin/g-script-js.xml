<glade-catalog
  name="g-script-js"
  version="1.0"
  icon-prefix="js"
  library="glade-g-script-js"
  domain="g"
  book="g-script-js">
  <glade-widget-classes>
    <glade-widget-class toplevel="True" name="GScriptJs" generic-name="script" title="Script">
      <create-editor-property-function>glade_g_script_js_create_eprop</create-editor-property-function>
      <!-- read-widget-function>glade_g_script_js_read_widget</read-widget-function -->
      <!-- write-widget-function>glade_g_script_js_write_widget</write-widget-function -->
      <set-property-function>glade_g_script_js_set_property</set-property-function>
      <properties>
      	<property id="filename" custom-layout="False" title="Source filename"/>
      	<property id="javascript" custom-layout="False" multiline="True" title="JavaScript code"/>
      </properties>
    </glade-widget-class>
  </glade-widget-classes>
  <glade-widget-group name="g-script-js" title="JavaScript Code">
    <glade-widget-class-ref name="GScriptJs"/>
  </glade-widget-group>
</glade-catalog>
